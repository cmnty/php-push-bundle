{
    "name": "cmnty/push-bundle",
    "description": "Symfony Bundle for cmnty/push",
    "keywords": ["Notifications", "Push Notifications", "Push Messages", "Push", "Web Push", "Web Push API", "Push API"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan de Ruijter",
            "email": "johan@cmnty.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "cmnty/push": "~0.0.1",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/yaml": "~2.8|~3.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.5"
    },
    "suggest": {
        "doctrine/orm": "Needed to store subscriptions using doctrine"
    },
    "autoload": {
        "psr-4": {
            "Cmnty\\PushBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
